<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension point="bndtools.core.cnfTemplates">
      <template
         id="bundle-hub"
         name="Bundle-Hub Configuration"
         doc="templateDocs/BundleHubConfig.xml"
         paths="templates/cnfs/bundlehub"
         priority="20"
         managed="ext/.*,plugins/.*"
         ignores="/cache/"
         version="0"
         category="AAA/Core"
      />
      <template
         id="jpm"
         name="JPM4J Configuration"
         doc="templateDocs/JpmConfig.xml"
         paths="templates/cnfs/jpm"
         priority="10"
         managed="ext/.*,plugins/.*"
         ignores="/cache/"
         version="10"
         category="AAA/Core"
      />
   </extension>

</plugin>
